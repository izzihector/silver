<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="report_product_qty_inventory">
		<t t-call="web.html_container">
			<t t-call="web.external_layout">
				<br/>

 

				<div class="text-right">
					<div class="page">
							<table  class="table table-bordered">
								<thead>
									<tr>
										<th style="text-align:center;width:15%">#</th>
										<th style="text-align:center;width:40%">Product</th>
										<th style="text-align:center;width:7%">Product Unit </th>
										<th style="text-align:center;width:15%">UOM</th>

										<th style="text-align:center;width:15%">Return SO</th>
										<th style="text-align:center;width:10%">Delivery SO</th>
										<th style="text-align:center;width:15%">Return PO</th>
										<th style="text-align:center;width:10%">Delivery PO</th>
										<th style="text-align:center;width:15%">Return Internal </th>
										<th style="text-align:center;width:10%">Delivery Imternal</th>
										<th style="text-align:center;width:15%">Manfurturing SO</th>
										<th style="text-align:center;width:10%">Manfurturing SO</th>


									</tr>
								</thead>
								<tbody>

						<t t-foreach="value_list" t-as="record">

							 


									<tr>
										 <td style="white-space: text-nowrap;text-align:center">
												<span t-esc="record[i]" />
										</td>
										<td style="white-space: text-nowrap;text-align:center">
												<span t-esc="record[product]" />
										</td>
										<td style="white-space: text-nowrap;text-align:center">
												<span t-esc="round(record[i],3)" />
										</td>
										<td style="white-space: text-nowrap;text-align:center">
												<span t-esc="round(record[return_so],3)" />
										</td>
										<td style="white-space: text-nowrap;text-align:center">
												<span t-esc="round(record[delivery_so],3)" />
										</td>
										<td style="white-space: text-nowrap;text-align:center">
												<span t-esc="round(record[return_po],3)" />
										</td>
										<td style="white-space: text-nowrap;text-align:center">
												<span t-esc="round(record[delivery_po],3)" />
										</td>
										<td style="white-space: text-nowrap;text-align:center">
												<span t-esc="round(record[return_internal],3)" />
										</td>
										<td style="white-space: text-nowrap;text-align:center">
												<span t-esc="round(record[delivery_internal],3)" />
										</td><td style="white-space: text-nowrap;text-align:center">
												<span t-esc="round(record[return_ma],3)" />
										</td>
										<td style="white-space: text-nowrap;text-align:center">
												<span t-esc="round(record[delivery_ma],3)" />
										</td>



							</tr>










						</t>
						</tbody>
							</table>
					</div>
				</div>
			</t>
		</t>
	</template>

</odoo>
