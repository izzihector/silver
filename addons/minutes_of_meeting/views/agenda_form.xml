<?xml version="1.0" encoding="utf-8"?>
<odoo>

<record id="wiz_genda_tree" model="ir.ui.view">
		<field name="name">wiz_genda_tree</field>
		<field name="model">calenar.agenda</field>
		
		<field name="arch" type="xml">
			<tree>
			  <field name="name"/>
			  <field name="code"/>
			</tree>
		</field>
	</record>



	<record id="wiz_genda_form" model="ir.ui.view">
		<field name="name">wiz_genda_form</field>
		<field name="model">calenar.agenda</field>
		
		<field name="arch" type="xml">
			<form>
			  <group>
			  <field name="name"/>
			  <field name="code"/>
			  </group>
			</form>
		</field>
	</record>


  
</odoo>